{
  "name": "@lenscape/semantics_check",
  "description": "type a query see the effectiveness",
  "version": "0.1.2",
  "browser": {
    "child_process": false
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@focuson/lens": "1.44.3",
    "@focuson/state": "1.44.3",
    "@laoban/utils": "1.4.45",
    "@lenscape/agents": "0.1.2",
    "@lenscape/azureai": "0.1.2",
    "@lenscape/llmselector": "0.1.2",
    "@lenscape/rag_agents": "0.1.2",
    "@lenscape/sample_agents": "0.1.2",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "axios": "1.7.2",
    "debug": "^4.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@parcel/packager-raw-url": "2.15.4",
    "@parcel/transformer-webmanifest": "2.15.4",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "parcel": "^2.15.4",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/",
    "build/"
  ],
  "keywords": [
    "react",
    "runbook"
  ],
  "license": "MIT",
  "peerDependencies": {},
  "repository": "git@github.com:intellimaintain/intellimaintain.git",
  "scripts": {
    "prepare-tiktoken": "copy ..\\..\\..\\node_modules\\tiktoken\\tiktoken_bg.wasm public\\",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest --config jest.config.json",
    "test:watch": "npm run test -- --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  }
}
